{
  "expo": {
    "name": "Urban Childhood",
    "slug": "urban-childhood",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#6C63FF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.urbanchildhood.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#6C63FF"
      },
      "package": "com.urbanchildhood.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "urbanchildhood",
    "plugins": [
      "expo-web-browser"
    ]
  }
}
